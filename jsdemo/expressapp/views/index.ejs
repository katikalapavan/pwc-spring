<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title>Document</title>
</head>
<body>
    <div class="container">
        <div>
            <form class="form" method="POST" action="/adduser">
                Name: <input type="text" class="form-control" name="uname">
                <br>
                Password: <input type="text" class="form-control" name="pass">
                <br>
                Email: <input type="text" class="form-control" name="email">
                <br>
                City: <input type="text" class="form-control" name="city">
                <br>
                <button class="btn-outline-success" >Register</button>
            </form>
        <div>

            <table class="table">
                
                <thead>
                  <tr>
                   
                    <th scope="col">name</th>
                    <th scope="col">pass</th>
                    <th scope="col">email</th>
                    <th scope="col">city</th>
                  </tr>
                </thead>

                <tbody>
             <% for(var i=0;i < udata.length;i++){
                    %>
                  <tr>
                     
                   <td> <%=udata[i].uname%></td>
                   <td> <%=udata[i].pass%></td>
                   <td> <%=udata[i].email%></td>
                   <td> <%=udata[i].city%></td>
                    
                  </tr>
                     
                   <% } %>
                </tbody>
               
              </table>
              
              
                
        </div>

</body>
</html>